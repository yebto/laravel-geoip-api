{
  "name": "yebto/laravel-geoip-api",
  "description": "Laravel SDK wrapper for the YEB GeoIP API. Fetch City, country or ASN information from any IP, with emoji flags.",
  "type": "library",
  "license": "MIT",
  "keywords": ["geoip", "ip", "asn", "country", "city", "laravel", "sdk", "yeb"],
  "homepage": "https://yeb.to",
  "support": {
    "email": "support@yeb.to",
    "issues": "https://github.com/yebto/laravel-geoip-api/issues",
    "source": "https://github.com/yebto/laravel-geoip-api"
  },
  "authors": [
    {
      "name": "NETOX Ltd.",
      "email": "support@yeb.to",
      "homepage": "https://yeb.to",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Yebto\\GeoIPAPI\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yebto\\GeoIPAPI\\GeoIPAPIServiceProvider"
      ],
      "aliases": {
        "GeoIPAPI": "Yebto\\GeoIPAPI\\Facades\\GeoIPAPI"
      }
    }
  }
}
